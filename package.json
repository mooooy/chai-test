{
  "name": "chai-test",
  "version": "1.0.0",
  "description": "Vite + Cycle.js + TypeScript project",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "build:test": "tsc -p tsconfig.test.json",
    "test:node": "mocha -r jsdom-global/register \"dist-test/test/**/*.js\" --exit",
    "test": "npm run build:test && npm run test:node",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "dependencies": {
    "@cycle/dom": "^23.1.0",
    "@cycle/run": "^5.7.0",
    "xstream": "^11.14.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.3.0",
    "chai": "^6.2.2",
    "chai-virtual-dom": "^1.0.0",
    "jsdom": "^28.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^11.7.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
